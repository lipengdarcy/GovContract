<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <!-- 
说明：两种运行方式
BAT运行脚本：java -jar mybatis-generator-core-1.3.2.jar -configfile generatorConfig.xml -overwrite
mvn命令：mvn mybatis-generator:generate
 -->
  <!-- 数据库驱动-->
  <classPathEntry location="C:\Users\lipeng\.m2\repository\mysql\mysql-connector-java\5.1.34\mysql-connector-java-5.1.34.jar"/>
  <context id="DB2Tables" targetRuntime="MyBatis3">
    <commentGenerator>
      <property name="suppressDate" value="true"/>
      <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>
    <!--数据库链接URL，用户名、密码 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost/witsafe" userId="root" password="123456"/>
    <javaTypeResolver>
      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>
    <!-- 生成模型的包名和位置-->
    <javaModelGenerator targetPackage="com.witsafe.model" targetProject="src\main\java">
      <property name="enableSubPackages" value="true"/>
      <property name="trimStrings" value="true"/>
    </javaModelGenerator>
    <!-- 生成映射文件的包名和位置-->
    <sqlMapGenerator targetPackage="com.witsafe.mapping" targetProject="src\main\java">
      <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    <!-- 生成DAO的包名和位置-->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.witsafe.dao" targetProject="src\main\java">
      <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    <!-- 要生成哪些表-->
    <table schema="hsshop" tableName="account"/>
    <table schema="hsshop" tableName="conf_dictionary"/>
    <table schema="hsshop" tableName="conf_dictitem"/>
    <table schema="hsshop" tableName="df_field"/>
    <table schema="hsshop" tableName="df_form"/>
    <table schema="hsshop" tableName="flow_approval"/>
    <table schema="hsshop" tableName="flow_borrow"/>
    <table schema="hsshop" tableName="nationstandard"/>
    <table schema="hsshop" tableName="organization"/>
    <table schema="hsshop" tableName="sec_authority"/>
    <table schema="hsshop" tableName="sec_authority_resource"/>
    <table schema="hsshop" tableName="sec_menu"/>
    <table schema="hsshop" tableName="sec_org"/>
    <table schema="hsshop" tableName="sec_resource"/>
    <table schema="hsshop" tableName="sec_role"/>
    <table schema="hsshop" tableName="sec_role_authority"/>
    <table schema="hsshop" tableName="sec_role_user"/>
    <table schema="hsshop" tableName="sec_user"/>
    <table schema="hsshop" tableName="sec_user_authority"/>
    <table schema="hsshop" tableName="sys_param"/>
    <table schema="hsshop" tableName="wf_cc_order"/>
    <table schema="hsshop" tableName="wf_hist_order"/>
    <table schema="hsshop" tableName="wf_hist_task"/>
    <table schema="hsshop" tableName="wf_hist_task_actor"/>
    <table schema="hsshop" tableName="wf_order"/>
    <table schema="hsshop" tableName="wf_process"/>
    <table schema="hsshop" tableName="wf_surrogate"/>
    <table schema="hsshop" tableName="wf_task"/>
    <table schema="hsshop" tableName="wf_task_actor"/>
  </context>
</generatorConfiguration>
